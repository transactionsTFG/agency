<?xml version="1.0" encoding='UTF-8'?>
  <faces-config
      xmlns="http://xmlns.jcp.org/xml/ns/javaee"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
      version="2.2">
 
 <!-- Rules pages error -->
 <navigation-rule>
	<from-view-id>/error.xhtml</from-view-id>
	<navigation-case>
		<from-outcome>index</from-outcome>
		<to-view-id>/public/alojamientos.xhtml</to-view-id>
	</navigation-case>
 </navigation-rule>

 <navigation-rule>
 	<from-view-id>/index.xhtml</from-view-id>
 	<navigation-case>
 		<from-outcome>alojamiento</from-outcome>
 		<to-view-id>public/alojamientos.xhtml</to-view-id>
 	</navigation-case>
 	<navigation-case>
 		<from-outcome>vuelo</from-outcome>
 		<to-view-id>public/vuelos.xhtml</to-view-id>
 	</navigation-case>
 	<navigation-case>
 		<from-outcome>vuelo-y-hotel</from-outcome>
 		<to-view-id>public/vuelo-y-hotel.xhtml</to-view-id>
 	</navigation-case>
 </navigation-rule>
 
 <navigation-rule>
 	<from-view-id>public/alojamientos.xhtml</from-view-id>
  	<navigation-case>
 		<from-outcome>alojamientos</from-outcome>
 		<to-view-id>alojamientos.xhtml</to-view-id>
 	</navigation-case>
 	<navigation-case>
 		<from-outcome>vuelos</from-outcome>
 		<to-view-id>vuelos.xhtml</to-view-id>
 	</navigation-case>
  	<navigation-case>
 		<from-outcome>vuelo-y-hotel</from-outcome>
 		<to-view-id>vuelo-y-hotel.xhtml</to-view-id>
 	</navigation-case>
 </navigation-rule>

 <navigation-rule>
 	<from-view-id>public/vuelos.xhtml</from-view-id>
  	<navigation-case>
 		<from-outcome>alojamientos</from-outcome>
 		<to-view-id>alojamientos.xhtml</to-view-id>
 	</navigation-case>
 	<navigation-case>
 		<from-outcome>vuelos</from-outcome>
 		<to-view-id>vuelos.xhtml</to-view-id>
 	</navigation-case>
  	<navigation-case>
 		<from-outcome>vuelo-y-hotel</from-outcome>
 		<to-view-id>vuelo-y-hotel.xhtml</to-view-id>
 	</navigation-case>
 </navigation-rule>
 
 <navigation-rule>
 	<from-view-id>public/vuelo-y-hotel.xhtml</from-view-id>
  	<navigation-case>
 		<from-outcome>alojamientos</from-outcome>
 		<to-view-id>alojamientos.xhtml</to-view-id>
 	</navigation-case>
 	<navigation-case>
 		<from-outcome>vuelos</from-outcome>
 		<to-view-id>vuelos.xhtml</to-view-id>
 	</navigation-case>
  	<navigation-case>
 		<from-outcome>vuelo-y-hotel</from-outcome>
 		<to-view-id>vuelo-y-hotel.xhtml</to-view-id>
 	</navigation-case>
 </navigation-rule>

</faces-config>